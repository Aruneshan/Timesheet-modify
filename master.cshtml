    <section id="sidebar">

      <ul class="side-menu top">
        <li class=" active ">
          <a href="#">
            <i class="bx bxs-dashboard">
            </i>
            <span class="text">Dashboard</span>
          </a>
          <li>
            <a href ="#">
              <i class="bx bxs-Time"></i>
              <span class="text"></span>
            </a>
          </li>
          <li>
			<li>
				<div>
					<h8>"Hi"@ViewBag.Session</h8>
					<h8>@ViewBag.Message</h8>
				</div>
			</li>
				
				<a href="#"><a asp-controller="" asp-action="">
					<i class='bx bxs-doughnut-chart' ></i>
					<span class="text">My Projects</span>
				</a>
				</a>
				<tr>
			</li>
			<li>
				<a href="#"><a asp-controller="" asp-action="">
					<i class='bx bxs-message-dots' ></i>
					<span class="text">TimeSheet</span>
				</a>
				</a>
			</li>
			<li>
				<a href="#"><a asp-controller="" asp-action="">
					<i class='bx bxs-group' ></i>
					<span class="text">Time Sheet Approval</span>
				</a>
			</li>
			<li>
				<a href="#"><a asp-controller="" asp-action="">
					<i class='bx bx-calendar' ></i>
					<span class="text">Leave</span>
				</a>
			</li>
			<li>
				<a href="#"><a asp-controller="" asp-action="">
					<i class='bx bx-shield-quarter'></i>
					<span class="text">Permission</span>
				</a>
			</li>
		</ul>
		<ul class="side-menu">
			<li>
				<a href="#"><a asp-controller="" asp-action="">
					<i class='bx bx-wrench' ></i>
					<span class="text">Settings</span>
				</a>

			</li>
			<li>
				<a href="#" class="logout"><a asp-controller="" asp-action="">
					<span class="text"><a asp-area="" asp-controller="" asp-action=""><i class='bx bx-log-out' ></i>Logout</a></span>
				</a>
			</li>

		</ul>
	</section>
	<section id="content">

		<nav>
			<i class='bx bx-menu' ></i>
			<a href="#" class="nav-link">Categories</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search' ></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="#" class="notification">
				<i class='bx bxs-bell' ></i>
				<span class=""></span>
			</a>
			
			<a href="#" class="profile">
				<img src="https://cdn-icons-png.flaticon.com/512/1144/1144760.png">
			</a>
		</nav>
		<main>
			<div class="head-title">
				<div class="left">
					<h1>Dashboard</h1>
					<ul class="breadcrumb">
						<li>
							<a href="#">Dashboard</a>
						</li>
						<li><i class='bx bx-chevron-right' ></i></li>
						<li>
							<a class="active" href="#">Home</a>
						</li>
					</ul>
				</div>
				<a href="#" class="btn-download">
					<i class="gg-export"></i>
					<span class="text">Export</span>
				</a>
			</div>

			<ul class="box-info">
				<li>
					<i class='bx bxs-calendar-check' ></i>
					<span class="text">
						<h3>1020</h3>
						<p>Task Approved</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-group' ></i>
					<span class="text">
						<p>Team Members</p>
						<h2></h2>
					</span>
				</li>
				<li>
					<i class='bx bxs-dollar-circle' ></i>
					<span class="text">
						<h3>$</h3>
						<p>Budget of Projects</p>
					</span>
				</li>
			</ul>
			<div class="table-data">
				<div class="order">
					<div class="head">
						<h3>Recent Orders</h3>
						<i class='bx bx-search' ></i>
						<i class='bx bx-filter' ></i>
					</div>
					<table>
						<thead>
							<tr>
								<th>User</th>
								<th>Date Order</th>
								<th>Status</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>
									<img src="img/people.png">
									<p>Arun</p>
								</td>
								<td>01-10-2022</td>
								<td><span class="status completed">Pending</span></td>
							</tr>
						</tbody>
					</table>
				</div>
				<div class="todo">
					<div class="head">
						<h3>Todos</h3>
						<i class='bx bx-plus' ></i>
						<i class='bx bx-filter' ></i>
					</div>
					<ul class="todo-list">
						<li class="completed">
							<p>Todo List</p>
							<i class='bx bx-dots-vertical-rounded' ></i>
						</li>
						<li class="completed">
							<p>Todo List</p>
							<i class='bx bx-dots-vertical-rounded' ></i>
						</li>
						<li class="not-completed">
							<p>Todo List</p>
							<i class='bx bx-dots-vertical-rounded' ></i>
						</li>
						<li class="completed">
							<p>Todo List</p>
							<i class='bx bx-dots-vertical-rounded' ></i>
						</li>
						<li class="not-completed">
							<p>Todo List</p>
							<i class='bx bx-dots-vertical-rounded' ></i>
						</li>
					</ul>
				</div>
			</div>
		</main>
	</section>
	<script src="script.js"></script>
</body>
</html>
        