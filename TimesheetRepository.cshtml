@{
    ViewData["Title"] = "TimesheetRepository";
}

<h1>Login List</h1>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                EmailId
            </th>
            <th>
                Password
            </th>
            <th>
                UserMode
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="25%">
                    @obj.WeekStartDate
                </td>
                <td width="25%">
                    @obj.DevelopmentHours
                </td>
                <td width="25%">
                    @obj.LearningHours
                </td>
                <td width="25%">
                    @obj.MeetingHours
                </td>
                <td width="25%">
                    @obj.MarketingHours
                </td>
                <td width="25%">
                    @obj.DocumentationHours
                </td>
                <td width="25%">
                    @obj.TotalHours
                </td>
            </tr>
        }
    </tbody>
</table>